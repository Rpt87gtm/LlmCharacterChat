<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat code coverage">
        <Configuration>
          <!-- Исключить все файлы в папке Migrations -->
          <ExcludeByFile>**/Migrations/*.cs</ExcludeByFile>
          <!-- Исключить файл Program.cs -->
          <ExcludeByFile>**/Program.cs</ExcludeByFile>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings>